#include <iostream>
using namespace std;
/*
                Find mountains - 1

        ● Read integers N, M, then Read matrix NxM. Print all positions that are mountain. Position is mountain if its value > 8 neighbours values
        ● Code smartly

        ● Input
            ○ 3 3
            ○ 8 6 1
            ○ 3 2 9
            ○ 1 6 4
        ● Output
            ○ 0 0 (8 > 6, 3, 2)
            ○ 1 2 (9 > 1, 2, 5, 4, 6)

*/

int main() {
    int row, col;
    cin >> row >> col;

    int di[] = { -1 , -1 , -1, 0 , 0, 1 , 1, 1 };
    int dj[] = { -1 , 0 , 1, -1 , 1, -1 , 0, 1 };

    int arr[row][col];
    for (int i = 0; i < row; ++i)
        for (int j = 0; j < col; ++j)
            cin >> arr[i][j];

    for (int i = 0; i < row; ++i) {
        for (int j = 0; j < col; ++j) {
            bool is_mountain = true;
            for (int k = 0; k < 8; ++k) {
                int new_i = i + di[k];
                int new_j = j + dj[k];
                if (new_i >= 0 && new_i < row && new_j >= 0 && new_j < col) {
                    if (arr[new_i][new_j] >= arr[i][j]) {
                        is_mountain = false;
                        break;
                    }
                }
            }
            if (is_mountain)
                cout << i << " " << j << endl;
        }
    }

    return 0;
}
